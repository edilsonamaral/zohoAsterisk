macro try-dial(provider, exten, ringtime) {
	Set(provider=${FILTER(0-9a-zA-Z,_-,${provider})});
	Set(exten=${FILTER(0-9,${exten})});
	Set(ringtime=${FILTER(0-9,${ringtime})});

	NoOp(Normalized exten='${exten}' ringtime='${ringtime}');
    
	if("${provider}" == "anveo") {
		Dial(SIP/0EDILS1${exten}@sbc.anveo.com,${ringtime},KTt);
	} else if("${provider}" == "voipinnovations") {
		Dial(SIP/1${exten}@voipinnovations,${ringtime},KTtr);
	} else if("${provider}" == "voipms") {
		Dial(SIP/1${exten}@voipms,${ringtime},KTtr);
	} else if("${provider}" == "bulkvs") {
		Dial(SIP/1${exten}@bulkvs,${ringtime},KTtr);
	}
	return;
};

